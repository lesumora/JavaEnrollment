<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="enrollmentsystem.LoginController">
    <stylesheets>
        <URL value="@/enrollmentsystem/css/styles.css" />
    </stylesheets>
    <children>
        <Pane layoutX="194.0" layoutY="160.0" prefHeight="250.0" prefWidth="250.0">
            <children>
                <Button fx:id="btnLogin" layoutX="172.0" layoutY="187.0" onAction="#handleButtonAction" prefHeight="32.0" prefWidth="78.0" text="Login" />
                <Label fx:id="label" layoutX="11.0" layoutY="35.0" minHeight="16" minWidth="69" prefWidth="73.0" text="Login">
                    <font>
                        <Font name="System Bold" size="19.0" />
                    </font>
                </Label>
                <TextField fx:id="tfUsername" layoutX="88.0" layoutY="83.0" prefHeight="32.0" prefWidth="163.0" promptText="username" />
                <PasswordField fx:id="pfPassword" layoutX="88.0" layoutY="127.0" prefHeight="32.0" prefWidth="163.0" promptText="password" />
                <Label layoutX="11.0" layoutY="90.0" text="Username" />
                <Label layoutX="11.0" layoutY="133.0" text="Password" />
                <Button fx:id="btnRegister" layoutX="10.0" layoutY="187.0" onAction="#handleButtonAction" prefHeight="32.0" prefWidth="78.0" text="Register" />
            </children>
        </Pane>
      <Button fx:id="btnExit" layoutX="612.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleButtonAction" text="X" />
    </children>
   
</AnchorPane>
