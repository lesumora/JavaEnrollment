<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="enrollmentsystem.LoginController">
   <children>
      <Pane layoutX="75.0" layoutY="75.0" prefHeight="250.0" prefWidth="250.0">
         <children>
            <Button fx:id="btnLogin" layoutX="157.0" layoutY="170.0" onAction="#handleButtonAction" prefHeight="32.0" prefWidth="93.0" text="Login" />
            <Label fx:id="label" layoutX="15.0" layoutY="48.0" minHeight="16" minWidth="69" prefWidth="73.0" text="Login">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <TextField fx:id="tfUsername" layoutX="75.0" layoutY="90.0" prefHeight="32.0" prefWidth="163.0" promptText="username" />
            <PasswordField fx:id="pfPassword" layoutX="75.0" layoutY="126.0" prefHeight="32.0" prefWidth="163.0" promptText="password" />
            <Label layoutX="13.0" layoutY="97.0" text="Username" />
            <Label layoutX="13.0" layoutY="133.0" text="Password" />
            <Button fx:id="btnRegister" layoutX="3.0" layoutY="170.0" onAction="#handleButtonAction" prefHeight="32.0" prefWidth="93.0" text="Register" />
         </children>
      </Pane>
   </children>
</AnchorPane>
